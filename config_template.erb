server {
	# basic setting
  server_name	<%= sub_domain %>.t-lab.cs.teu.ac.jp;
  include "conf/includes/lets-http.conf";
}

server {

	# basic setting
  server_name	<%= sub_domain %>.t-lab.cs.teu.ac.jp;

	# access log
	access_log off;
	error_log on;

	# SSL&Auth setting
  include "conf/includes/lets.conf";
  include "conf/includes/internal.conf";
	include "conf/includes/header.conf";

	# Reverse Proxy
	location / {
  proxy_pass http://<%= sub_domain %>.<%= internal_address %>;
	}
}

